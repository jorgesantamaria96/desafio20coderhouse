<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Info - Desafio 20 Coder</title>
    <script
      src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js"
      integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p"
      crossorigin="anonymous"
    ></script>
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="./index.css" />
  </head>
  <body>

    <%- include('navbar'); -%>

    
    <div class="info__container">
      <div class="info__card_content">
        <% args.forEach(function(arg, index){ %>
          <div class="info__card">
            <p class="info__title">Arg <%= index %>: </p>
            <p class="info__value"><%= arg %></p>
          </div>
        <% }) %>

        <div class="info__card">
          <p class="info__title">CPU's: </p>
          <p class="info__value"><%= numCPUS %></p>
        </div>

        <div class="info__card">
          <p class="info__title">Absolute Route: </p>
          <p class="info__value"><%= absoluteRoute %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Pid: </p>
          <p class="info__value"><%= pid %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Version: </p>
          <p class="info__value"><%= version %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Title: </p>
          <p class="info__value"><%= title %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Platform: </p>
          <p class="info__value"><%= platform %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Rss: </p>
          <p class="info__value"><%= memoryUsage.rss %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Heap Total: </p>
          <p class="info__value"><%= memoryUsage.heapTotal %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Heap Used: </p>
          <p class="info__value"><%= memoryUsage.heapUsed %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">External: </p>
          <p class="info__value"><%= memoryUsage.external %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Array Buffers: </p>
          <p class="info__value"><%= memoryUsage.arrayBuffers %></p>
        </div>
  
        <div class="info__card">
          <p class="info__title">Exec Path: </p>
          <p class="info__value"><%= execPath %></p>
        </div>
      </div>

    </div>

  </div>

  <script src="/socket.io/socket.io.js"></script>
  <script src="./productos.js"></script>
  </body>
</html>
